#设置对输出参数的格式化
spring.jackson.date-format=yyyy-MM-dd HH:mm:ss

#北京时间相对伦敦有8个小时时差所以使用GMT+8，这样才能正常显示日期
spring.jackson.time-zone=GMT+8

#设置对输入参数的格式化，这样后台的接口就可以接收Date类型
spring.mvc.date-format=yyyy-MM-dd

#设置端口号
server.port=8888

#设置前缀
server.servlet.context-path=/

#设置自己的文件服务器地址
fileServer.path=http://image.qianfeng.com

#设置自己的邮箱服务器地址
emailServer.path=http://email.qianfeng.com

#设置系统相关资源的信息
resource.imageServer = http://images.lzp.com
resource.emailServer = http://email.lzp.com
